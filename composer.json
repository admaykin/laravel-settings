{
  "name": "admaykin/laravel-settings",
  "description": "Simple Settings package for laravel application",
  "keywords": [
    "Laravel",
    "Settings"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Max Admaykin",
      "email": "admaykin@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ADMAYKIN\\Settings\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6.4"
  },
  "extra": {
    "laravel": {
      "providers": [
        "ADMAYKIN\\Settings\\App\\Providers\\SettingServiceProvider"
      ],
      "aliases": {
        "Settings": "ADMAYKIN\\Settings\\App\\Facades\\Setting"
      }
    }
  }
}
